{
    "name": "fivem/clashofclans",
    "description": "API client for the ClashOfClans game",
    "type": "library",
    "keywords": ["clashofclans","client","api"],
    "homepage": "https://github.com/xavhermans/clashofclans",
    "license": "MIT",
    "authors": [
        {
            "name": "Xavier Hermans",
            "email": "xav.hermans@gmail.com",
            "homepage": "https://github.com/xavhermans",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Fivem\\ClashOfClans\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ClashOfClans\\Tests\\": "tests/"
        }
    },
    "require": {
        "symfony/http-client": "^5.1",
        "phpunit/phpunit": "^9.3",
        "php": "^7.4",
        "symfony/serializer": "^5.1",
        "symfony/property-access": "^5.1",
        "doctrine/collections": "^1.6",
        "doctrine/annotations": "^1.10"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "config": {
        "preferred-install": "dist"
    }
}
